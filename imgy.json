{
    "host": "localhost",
    "port": "8080",
    "databaseType": "sqlite",
    "database": {
        "path": "imgy.db"
    },
    "aws": {
        "bucket": "imgy-s3",
        "region": "us-west-2"
    },
    "tlsEnabled": false,
    "tls": {
        "cert": "/path/to/cert",
        "key": "/path/to/key"
    },
    "cookieStoreKey": "something long and complex.",
    "inviteOnly": true,
    "oauthUrl": "http://localhost:8080",
    "oauthProviders": [
        "google"
    ],
    "oauthSettings": {
       "google": {
            "clientId": "",
            "clientSecret": "",
            "redirectPath": "/auth/callback/google"
        },
        "github": {
            "clientId": "",
            "clientSecret": "",
            "redirectPath": "/auth/callback/github"
        }
    }
}